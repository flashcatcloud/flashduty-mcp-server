  "method": "tools/call",
  "params": {
    "name": "flashduty_create_channel",
    "arguments": {
      "name": "New Channel",
      "type": "webhook",
      "webhook_url": "https://example.com/webhook"
    }
  }
}

RESPONSE:
{
  "content": [
    {
      "type": "text",
      "text": "{\"channel_id\":\"ch3\",\"name\":\"New Channel\",\"type\":\"webhook\"}"
    }
  ],
  "isError": false
} 